{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 */6 * * *"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "direction": "out",
      "path": "blog-data/{rand-guid}.json",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
